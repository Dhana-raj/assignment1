#include <stdio.h>
void main() {
    int arr[10]={1,0,2,0,3,0,4,0,5};
    int i,count=0;
   int n=10;
   for(i=0;i<n;i++) {
       if(arr[i]>0){
         arr[count++]=arr[i];
       }
   }
    while(count<n){
       arr[count++]=0;
   }
    for(i=0;i<n;i++) {
       printf("%d\t",arr[i]);
   }
}     
